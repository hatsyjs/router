{
  "name": "@hatsy/router",
  "version": "2.3.1",
  "description": "Hatsy router",
  "keywords": [
    "http-middleware",
    "http-router",
    "routing"
  ],
  "homepage": "https://github.com/hatsyjs/router",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:hatsyjs/router.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/hatsyjs/router/issues"
  },
  "type": "module",
  "types": "./dist/router.d.ts",
  "exports": {
    "types": "./dist/router.d.ts",
    "default": "./dist/router.js"
  },
  "sideEffects": false,
  "dependencies": {
    "@hatsy/http-header-value": "^3.7.1",
    "@hatsy/route-match": "^1.4.2",
    "@proc7ts/primitives": "^3.0.2"
  },
  "peerDependencies": {
    "@hatsy/hatsy": "^2.5.3"
  },
  "devDependencies": {
    "@hatsy/hatsy": "^2.5.3",
    "@jest/globals": "^29.6.2",
    "@proc7ts/logger": "^1.3.2",
    "@run-z/eslint-config": "^3.5.0",
    "@run-z/prettier-config": "^2.0.0",
    "@run-z/project-config": "^0.19.3",
    "@swc/core": "^1.3.75",
    "@swc/jest": "^0.2.28",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "eslint-plugin-jest": "^27.2.3",
    "gh-pages": "^5.0.0",
    "jest": "^29.6.2",
    "jest-junit": "^16.0.0",
    "jest-mock": "^29.6.2",
    "prettier": "^2.8.8",
    "prettier-eslint-cli": "^7.1.0",
    "rollup": "^3.27.2",
    "run-z": "^1.11.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.1",
    "typedoc": "^0.24.8",
    "typescript": "~5.1.6"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then build-z",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then clean-z",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "format": "run-z +z --then prettier-eslint --write --include-dot-files \"src/**/*.*\" \"*.{js,cjs,json,md}\"",
    "lint": "run-z +z --then eslint .",
    "test": "run-z +z env:NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" --then test-z",
    "z": "run-z +cmd:build-z,+cmd:typedoc,+cmd:eslint,+cmd:test-z"
  }
}
